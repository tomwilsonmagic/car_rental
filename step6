<script>
	
	$(document).ready(function(){  
		
		var	move_house	=	$('input[name="move_house"]:checked').val();
			
			if( move_house == 'Yes' ){
				
				$('.newaddressfield').css('display', 'block') ;
					
			}else{
				
				$('.newaddressfield').css('display', 'none') ;
			}
			
		var	own_insurance	=	$('input[name="own_insurance"]:checked').val();
		
			if( own_insurance == 'Yes' ){
				
				$('.national_insurance').css('display', 'block') ;
					
			}else{
				
				$('.national_insurance').css('display', 'none') ;
			}
		
	}) ;

	
	
	$( document ).on('click', '.move_house', function(){
			
			var	move_house	=	$('input[name="move_house"]:checked').val();

			if( move_house == 'Yes' ){
				
				$('.newaddressfield').css('display', 'block') ;
					
			}else{
				
				$('.newaddressfield').css('display', 'none') ;
			}
	})		
	
	$( document ).on('click', '.own_insurance', function(){
			
			
			var	own_insurance	=	$('input[name="own_insurance"]:checked').val();

			if( own_insurance == 'Yes' ){
				
				$('.national_insurance').css('display', 'block') ;
					
			}else{
				
				$('.national_insurance').css('display', 'none') ;
			}
	})		
	
	
	
	$( document ).on('click', '#charge-5', function(){
		
		var isChecked = $(this).prop("checked") ? 1 : 0 ;
		
		
		 if(!isChecked){
			  // alert('they must use their own insurance?') ;
			  swal({
			  title: "Are you sure?",
			  text: "You must use your own insurance?",
			  icon: "warning",
			  buttons: true,
			  dangerMode: true,
			})
			.then((willDelete) => {
				
				if (willDelete) {
				swal('You must use your own insurance?');
				
				} else {
					
					$( "#charge-5" ).prop( "checked", true );

					swal("Great", {
					  icon: "success",
					});
				}
			  
			});

		 }
	})
	

	$(document).on('click', '#sbtd1', function(event){
			event.preventDefault() ;
		var spinner = $('#loader');
		isValidate	=	formValidateStep1() ;
		
		if( isValidate ){
			
			spinner.show();
			$("form#step1Form").submit();
		}
		
	})
	
	
 
 
 function formValidateStep1(){
	  
	  var isChecked = $('#agree').prop("checked") ? 1 : 0 ;
	  

		 
	  if( $("#pick_up_date").val() == ''){
		  $('.pick_up_date-error').text('Pick-up date is required') ;
		  return false ;
	  }else{
		 $('.pick_up_date-error').text('') ;
	  }
	  
	  
	  if( $("#return_date").val() == ''){
		  $('.return_date-error').text('Return-up date is required') ;
		  return false ;
	  }
	  else{
		  $('.return_date-error').text('') ;
	  }
	   
	   
	  if( $("#pick_up_time").val() == ''){
		  $('.pick_up_time-error').text('Pick-up time is required') ;
		  return false ;
	  }else{
		   $('.pick_up_time-error').text('') ;
	  }
	  
	  
	  if( $("#return_time").val() == ''){
		  $('.return_time-error').text('Return-up time is required') ;
		  return false ;
	  }else{
		   $('.return_time-error').text('') ;
	  }
	  
	  if(isChecked == 0){
		 $('.agree-error').text('Is required') ;
		
			return false ;
		}else if(isChecked == 1){
			   // $('#sbtd1').removeAttr('disabled');
			 $('.agree-error').text('') ;
			
		}
	  
	  return true ;
	  
  }
  
  
  



$(function() {
    $( "#pick_up_date" ).datepicker({
	  
	  beforeShowDay: function(date) {
		var day = date.getDay();
		return [day != 0,''];
	  },
      showOn: "button",
	  dateFormat:  'yy-mm-dd',
      buttonImage: "http://jqueryui.com/resources/demos/datepicker/images/calendar.gif",
      buttonImageOnly: true,
	              minDate: 0,
            onSelect: function (date) {
                var dt2 = $('#return_date');
                var startDate = $(this).datepicker('getDate');
                var minDate = $(this).datepicker('getDate');
                dt2.datepicker('setDate', minDate);
                startDate.setDate(startDate.getDate() + 30);
                //sets dt2 maxDate to the last day of 30 days window
                dt2.datepicker('option', 'maxDate', startDate);
                dt2.datepicker('option', 'minDate', minDate);
                $(this).datepicker('option', 'minDate', minDate);
            }
			
    });
   $( "#return_date" ).datepicker({
	  
	  beforeShowDay: function(date) {
		var day = date.getDay();
		return [day != 0,''];
	   },
      showOn: "button",
	  dateFormat: 'yy-mm-dd',
	  buttonImage: "http://jqueryui.com/resources/demos/datepicker/images/calendar.gif",
      buttonImageOnly: true
    });  

	$("#birthdate").datepicker({ 
		changeYear: true, 
		// yearRange : 'yy-50:yy+1',
		yearRange: "-80:+0", // this is the option you're looking for
		dateFormat: 'yy-mm-dd',
		showOn: "both", 
		buttonImage: "http://jqueryui.com/resources/demos/datepicker/images/calendar.gif",
		buttonImageOnly: true 
	});
	
	$("#dr_licno_issuedate").datepicker({ 
		changeYear: true, 
		// yearRange : 'yy-50:yy+1',
		yearRange: "-50:+0", // this is the option you're looking for
		dateFormat: 'yy-mm-dd',
		showOn: "both", 
		buttonImage: "http://jqueryui.com/resources/demos/datepicker/images/calendar.gif",
		buttonImageOnly: true 
	});


	$("#dr_licno_expiredate").datepicker({ 
		changeYear: true, 
		// yearRange : 'yy-50:yy+1',
		yearRange: "1980:2025",
		// yearRange: "-50:+0", // this is the option you're looking for
		dateFormat: 'yy-mm-dd',
		showOn: "both", 
		buttonImage: "http://jqueryui.com/resources/demos/datepicker/images/calendar.gif",
		buttonImageOnly: true 
	});

	
  });
  </script>

  <script>
	
	$(document).on('click', '#sbt7d', function(event){
			event.preventDefault() ;
	 var isChecked = $('#iagree').prop("checked") ? 1 : 0 ;
			  
	  if(isChecked == 0){
		 $('.agree-error').text('Is required') ;
			return false ;
		}else if(isChecked == 1){
			 
			 $('.agree-error').text('') ;
			 $("form#step6Form").submit();
		}

		
	})
	
	$(document).on('click', '#sbt5', function(event){
			event.preventDefault() ;
		var spinner = $('#loader');
		isValidate	=	formValidateStep4() ;
		
		if( isValidate ){
			spinner.show();
			$("form#customerForm").submit();
		}	
	})
	
	function formValidateStep4(){
		
		
		

	   if( $("#first_name").val() == ''){
		  $('.first_name-error').text('First name is required') ;
		
		var scrollDiv = document.getElementById("first_name").offsetTop;
		window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
  
			return false ;
		}else{
		   $('.first_name-error').text('') ;
		}
		
		if( $("#last_name").val() == ''){
		  $('.last_name-error').text('Last name is required') 
		  
		  var scrollDiv = document.getElementById("last_name").offsetTop;
		window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
		
		  return false ;
		}else{
		   $('.last_name-error').text('') ;
		}
		
		if( $("#email").val() == ''){
		  $('.email-error').text('Email is required') ;
		  
		    var scrollDiv = document.getElementById("email").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
		
		  return false ;
		}else{
		   $('.email-error').text('') ;
		}
		
		if( $("#phone").val() == ''){
		  $('.phone-error').text('Phone is required') ;
		  
		      var scrollDiv = document.getElementById("phone").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.phone-error').text('') ;
		}
		
		if( $("#address").val() == ''){
		  $('.address-error').text('address is required') ;
		  
		        var scrollDiv = document.getElementById("address").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.address-error').text('') ;
		}
		
		if( $("#street2").val() == ''){
		  $('.street2-error').text('Street-2 is required') ;
		  
			var scrollDiv = document.getElementById("street2").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.street2-error').text('') ;
		}
		
		if( $("#street3").val() == ''){
		  $('.street3-error').text('Street-3 is required') ;
		  
		  		var scrollDiv = document.getElementById("street3").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.street3-error').text('') ;
		}
		
		if( $("#city").val() == ''){
		  $('.city-error').text('City is required') ;
		  
		  	var scrollDiv = document.getElementById("city").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.city-error').text('') ;
		}	

		if( $("#postal_code").val() == ''){
		  $('.postal_code-error').text('Postal code is required') ;
		  
		    	var scrollDiv = document.getElementById("postal_code").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.postal_code-error').text('') ;
		}

		if( $("#birthdate").val() == ''){
		  $('.birthdate-error').text('Birthdate is required') ;
		  
		   	var scrollDiv = document.getElementById("birthdate").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.birthdate-error').text('') ;
		}	

		if( $("#occupation").val() == ''){
		  $('.occupation-error').text('Occupation is required') ;
		  
		  		   	var scrollDiv = document.getElementById("occupation").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.occupation-error').text('') ;
		}	

		if( $("#second_form_file").val() == ''){
		  $('.second_form_file-error').text('Second form Id is required') ;
		  
		    	var scrollDiv = document.getElementById("second_form_file").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.second_form_file-error').text('') ;
		}	

		if( $("#dr_licno").val() == ''){
		  $('.dr_licno-error').text('Driving license number is required') ;
		  
		  	var scrollDiv = document.getElementById("dr_licno").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.dr_licno-error').text('') ;
		}
		
		if( $("#dr_licno_file").val() == ''){
		  $('.dr_licno_file-error').text('Driving license file is required') ;
		  
		    	var scrollDiv = document.getElementById("dr_licno_file").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.dr_licno_file-error').text('') ;
		}	

		if( $("#dr_licno_issuedate").val() == ''){
		  $('.dr_licno_issuedate-error').text('Driving license issuedate is required') ;
		  
		  		    	var scrollDiv = document.getElementById("dr_licno_issuedate").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.dr_licno_issuedate-error').text('') ;
		}	

		if( $("#dr_licno_expiredate").val() == ''){
		  $('.dr_licno_expiredate-error').text('Driving license expiredate is required') ;
		  
		  	    	var scrollDiv = document.getElementById("dr_licno_expiredate").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.dr_licno_expiredate-error').text('') ;
		}

var	own_insurance	=	$('input[name="own_insurance"]:checked').val();
		
	if(  own_insurance != 'Yes' && own_insurance != 'No' ){
		  $('.own_insurance-error').text('Own insurance option is required') ;
		 
		  	var scrollDiv = document.getElementsByClassName("own_insurance").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.own_insurance-error').text('') ;
	
	}	
		
		
	if( $("#insurance_policy_file").val() == '' &&  own_insurance == 'Yes'){
		  $('.insurance_policy_file-error').text('Insurance policy is required') ;
		  
		  	var scrollDiv = document.getElementById("insurance_policy_file").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.insurance_policy_file-error').text('') ;
		}	

	var	move_house	=	$('input[name="move_house"]:checked').val();

	
		if(  move_house != 'Yes' && move_house != 'No'){
		  $('.move_house-error').text('Own insurance option is required') ;
		  
		  	var scrollDiv = document.getElementsByClassName("move_house").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.move_house-error').text('') ;
	
	}
	
		if( $("#new_address").val() == '' &&  move_house == 'Yes'){
		  $('.new_address-error').text('New address is required') ;
		  
		  		  	var scrollDiv = document.getElementById("new_address").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.new_address-error').text('') ;
		}	

		if( $("#driving_offence").val() == ''){
		  $('.driving_offence-error').text('Driving offence field is required') ;
		  
		  		  
		  		  	var scrollDiv = document.getElementById("driving_offence").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.driving_offence-error').text('') ;
		}	

		if( $("#driving_accidents").val() == ''){
		  $('.driving_accidents-error').text('Driving accidents field is required') ;
		  
		  		  	var scrollDiv = document.getElementById("driving_accidents").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.driving_accidents-error').text('') ;
		}	

		if( $("#bill_statement").val() == ''){
		  $('.bill_statement-error').text('Bill statement is required') ;
		  
		   	var scrollDiv = document.getElementById("bill_statement").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.bill_statement-error').text('') ;
		}	

		if( $("#disablities").val() == ''){
		  $('.disablities-error').text('Disablities field is required') ;
		  
		    	var scrollDiv = document.getElementById("disablities").offsetTop;
			window.scrollTo({ top: scrollDiv, behavior: 'smooth'});
			
		  return false ;
		}else{
		   $('.disablities-error').text('') ;
		}	

		
		return true ;
	}
  </script>
  
  <div class="contact-modal">
    <div class="modal-wrapper">
	<i class="fa fa-times" aria-hidden="true"></i>
	    <form class="">
				  <h3> Send Email </h3>
				  <p>The selected vehicle class is currently not available. Please leave your email address so we can contact you about alternatives. </p>
				  <div class="form-group">
					 <label> Email Address </label>
					  <input class=" form-control" no_name_field="" id="email_address" name="email_address" type="text" value="">
				  </div>
			 
		
		<div class="btn-row">
		<a >Cancel</a>
		    <input type="submit" class="sbt-modal" value=" Send Email">
		</div>
	    </form>
		
	</div>
  
  </div>
  
  
  
  
  <script>
$(document).ready(function(){
  $(".rentbutton.cn-button").click(function(){
    $(".contact-modal").addClass('Modal-show');
  });
  
   $(".fa.fa-times").click(function(){
    $(".contact-modal").removeClass('Modal-show');
  });
  
   $(".modal-wrapper .btn-row a").click(function(){
    $(".contact-modal").removeClass('Modal-show');
  });
});
</script>
